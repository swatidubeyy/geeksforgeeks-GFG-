// page source --->>>>> https://practice.geeksforgeeks.org/problems/largest-element-in-array4009/1/?page=2&category[]=Arrays&sortBy=submissions#

int largest(int arr[], int n) {
    int max = 0;
    for(int i = 0 ; i < n ; i++){
        if(arr[i] > max){
            max = arr[i];
        }
    }
    return max;
}
